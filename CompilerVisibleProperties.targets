<Project>
  <Target Name="RemoveDuplicateProjectProperties" BeforeTargets="InjectThisAssemblyProject">
    <ItemGroup>
      <DistinctProjectProperty Include="@(ProjectProperty->Distinct())" />
      <ProjectProperty Remove="@(ProjectProperty)" />
      <ProjectProperty Include="@(DistinctProjectProperty)" />
    </ItemGroup>
  </Target>
</Project>
