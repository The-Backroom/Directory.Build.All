<Project>
  <PropertyGroup>
    <AssetTargetFallback Condition="'$(TargetFramework)' == 'net7.0'">net6.0;netcoreapp3.1;netstandard2.1;netstandard2.0</AssetTargetFallback>
    <AssetTargetFallback Condition="'$(TargetFramework)' == 'net6.0'">netcoreapp3.1;netstandard2.1;netstandard2.0</AssetTargetFallback>
    <AssetTargetFallback Condition="'$(TargetFramework)' == 'netstandard2.1'">netstandard2.0</AssetTargetFallback>
    <AssetTargetFallback Condition="'$(TargetFramework)' == 'netstandard2.0'">netstandard1.3</AssetTargetFallback>
  </PropertyGroup>
</Project>
